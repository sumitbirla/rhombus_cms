<div class="pull-right">
  <%= link_to new_admin_cms_list_path, class: 'btn btn-default btn-sm' do %>
    <i class="fa fa-plus"></i> New list
  <% end %>
</div>
<h2><i class="fa fa-tags"></i>&nbsp; Lists</h2>


<%= will_paginate @lists %>
<table class="table table-striped table-condensed">
  <tr>
    <th>Owner</th>
    <th>Name</th>
    <th>Values</th>
    <th></th>
  </tr>
  <%= render @lists %>
</table>


<%= render 'admin/shared/table_summary', data: @lists %>


<%= content_for :title, "Lists" %>


<% content_for :breadcrumbs do %>
  <li><a href="#">CMS</a></li>
  <li class="active">Lists</li>
<% end %>